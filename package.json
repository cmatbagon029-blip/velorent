{
  "name": "velorent",
  "version": "1.0.0",
  "description": "Velorent - Vehicle Rental Application",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently -n \"backend,frontend\" -c \"yellow,blue\" \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && node app.js",
    "frontend": "cd velorent-app && ionic serve",
    "backend:dev": "cd backend && node app.js",
    "frontend:dev": "cd velorent-app && ionic serve",
    "install:all": "npm install && cd backend && npm install && cd ../velorent-app && npm install",
    "build:android": "cd velorent-app && npm run build:prod && npx cap sync",
    "get-ip": "cd velorent-app && powershell -ExecutionPolicy Bypass -File get-ip.ps1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "vehicle",
    "rental",
    "ionic",
    "angular"
  ],
  "author": "",
  "license": "ISC"
}
